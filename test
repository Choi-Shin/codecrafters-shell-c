echo "example'world'\\n'shell"
# example'world'\n'shell

echo "example\"insidequotes"world\"
# example"insidequotesworld"

echo "mixed\"quote'test'\\"
# mixed"quote'test'\

echo 'script\\\ntest'
# script\\\ntest

echo "before\   after"
# before\   after

echo "shell  hello"  "script"
shell  hello script